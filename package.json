{
  "name": "vigilance-conventionnal",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "",
  "main": "dist/main.js",
  "scripts": {
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "ci:jest": "jest",
    "ci:prettier": "prettier --check ./**/*.{js,ts,json,vue,html,css,less}",
    "ci:tsc": "tsc --noEmit",
    "ci:xo": "xo src",
    "fix:prettier": "prettier --write ./**/*.{js,ts,json,vue,html,css,less}",
    "start": "node dist/main.js commit",
    "unit:coverage": "jest --coverage",
    "unit:watch": "jest --watch",
    "prewatch": "rimraf dist",
    "watch": "webpack --watch --mode development"
  },
  "dependencies": {
    "caporal": "^1.4.0",
    "iconv-lite": "^0.5.1",
    "inquirer": "^7.1.0",
    "inversify": "^5.0.1",
    "inversify-binding-decorators": "^4.0.0",
    "inversify-inject-decorators": "^3.1.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "babel-loader": "^8.1.0",
    "cache-loader": "^4.1.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "thread-loader": "^2.1.3",
    "ts-jest": "^25.5.1",
    "ts-loader": "^7.0.4",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "xo": "^0.30.0"
  },
  "lint-staged": {
    "**/*.*": [
      "npm run ci:prettier"
    ],
    "**/*.ts": [
      "npm run ci:tsc",
      "npm run ci:xo"
    ],
    "**/*.js": [
      "npm run ci:xo"
    ]
  },
  "husky": {
    "pre-commit": "lint-staged"
  }
}
